{
  "name": "node-reqwest",
  "version": "0.0.0",
  "description": "Node.js bindings for reqwest - Rust HTTP client library",
  "keywords": [
    "fetch",
    "http",
    "https",
    "client",
    "http client",
    "request",
    "reqwest",
    "node"
  ],
  "author": "Vadim Piven <vadim@piven.tech> (https://piven.tech)",
  "homepage": "https://github.com/vadimpiven/node_reqwest",
  "bugs": {
    "url": "https://github.com/vadimpiven/node_reqwest/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadimpiven/node_reqwest.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0 OR MIT",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "type": "module",
  "types": "./export_dist/index.d.ts",
  "main": "./export_dist/index.js",
  "exports": {
    ".": {
      "types": "./export_dist/index.d.ts",
      "import": "./export_dist/index.js",
      "require": "./export_dist/index.cjs"
    }
  },
  "files": [
    "export/**/*",
    "export_dist/**/*"
  ],
  "binary": {
    "module_name": "node_reqwest",
    "module_path": "./dist/napi-v{napi_build_version}/",
    "host": "https://github.com/",
    "remote_path": "./vadimpiven/{module_name}/releases/download/v{version}/",
    "package_name": "{module_name}-v{version}-napi-v{napi_build_version}-{platform}-{arch}.tar.gz",
    "napi_versions": [
      8
    ]
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "catalog:",
    "undici": "catalog:"
  },
  "devDependencies": {
    "@codecov/vite-plugin": "catalog:",
    "@playwright/test": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "electron": "catalog:",
    "monocart-reporter": "catalog:",
    "rollup-plugin-node-externals": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "scripts": {
    "bump-version": "cargo run -p meta --bin npm_version -r --locked",
    "type-check": "pnpm exec tsc --noEmit",
    "build:cargo": "cargo build",
    "build:ts": "vite build && pnpm pack --out package.tar.gz",
    "build": "pnpm run build:cargo && pnpm run build:ts",
    "test:vitest": "pnpm exec vitest run --coverage",
    "test:playwright": "node ../../scripts/test-playwright.ts --config playwright.config.ts",
    "test:ts": "pnpm run type-check && pnpm run test:vitest && pnpm run test:playwright",
    "test:nextest": "node ../../scripts/test-nextest.ts",
    "test:rustdoc": "cargo doc --no-deps --document-private-items",
    "test:rust": "pnpm run test:nextest && pnpm run test:rustdoc",
    "pretest": "pnpm run build",
    "test": "pnpm run test:ts && pnpm run test:rust",
    "ci-build": "pnpm run build:cargo -r --locked && pnpm run build:ts",
    "preci-test": "pnpm run ci-build",
    "ci-test:rust": "pnpm run test:nextest -r --locked && pnpm run test:rustdoc -r --locked",
    "ci-test": "pnpm run test:ts && pnpm run ci-test:rust",
    "pack-addon": "pnpm exec node-pre-gyp package",
    "install:skip": "node -p \"process.exit(+!(require('./package.json').version === '0.0.0'))\"",
    "install": "node-pre-gyp install --update-binary --fallback-to-build=false || pnpm run install:skip"
  }
}
