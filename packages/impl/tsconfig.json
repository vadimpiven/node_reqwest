{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "ESNext.Symbol", "ESNext.Disposable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "outDir": "./export_dist"
  },
  "include": ["./export/**/*.ts", "./tests/**/*.{mjs,ts}"]
}
